#include <iostream>
#include <cmath>
using namespace std;

int main() {
	setlocale(LC_ALL, "rus");
	int n;
        cout << "Введите кол-во цифр ";
	cin >> n;
	int k = 0;
	int a;

	while (n > 0) {
		cin >> a; //ввод чисел
		k += 1;
		if (a<0){
			cout << "Отрицательное число под номером " << k;
			break;
		}
		n -= 1;
	}
	return 0;
}
